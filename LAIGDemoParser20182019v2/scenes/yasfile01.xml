<yas>

	<scene root="child_room" axis_length="5" />

	<views default="perspective0">

		<perspective id="perspective0" near="0.1" far="500" angle="0.4">
			<from x="15" y="15" z="15" />
			<to x="0" y="0" z="0" />
		</perspective>

		<ortho id="ortho0" near="0.1" far="500.0" left="15" right="15" top="15" bottom="15" />

	</views>

	<ambient> 
		<ambient r="0.5" g="0.5" b="0.5" a="1.0" /> 
		<background r="0" g="0" b="0" a="1" /> 
	</ambient> 

	<lights>

		<!-- Omni Lights -->
		<omni id="light0" enabled="0">
			<location x="4.0" y="6.0" z="1.0" w="1.0"/>
			<ambient r="0.0" g="0.0" b="0.0" a="1.0"/>
			<diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
			<specular r="1.0" g="1.0" b="0.0" a="1.0" />
		</omni>

		<omni id="light1" enabled="1">
			<location x="10.5" y="6.0" z="1.0" w="1.0"/>
			<ambient r="0.0" g="0.0" b="0.0" a="1.0"/>
			<diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
			<specular r="0.0" g="0.0" b="0.0" a="0.0" />
		</omni>

		<omni id="light2" enabled="0">
			<location x="8.0" y="7.0" z="1.0" w="1.0"/>
			<ambient r="0.0" g="0.0" b="0.0" a="1.0"/>
			<diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
			<specular r="1.0" g="1.0" b="1.0" a="1.0" />
		</omni>

		<omni id="light3" enabled="1">
			<location x="4.0" y="6.0" z="5.0" w="1.0"/>
			<ambient r="0.0" g="0.0" b="0.0" a="1.0"/>
			<diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
			<specular r="1.0" g="1.0" b="0.0" a="1.0" />
		</omni>

		<!-- Spot Lights -->
		<spot id="light4" enabled="0" angle="0.5" exponent="1.0" >
			<location x="0" y="0" z="0" w="0" />
			<target x="0" y="0" z="0" />
			<ambient r="0.1" g="0.1" b="0.1" a="0.1" />
			<diffuse r="0.1" g="0.1" b="0.1" a="0.1" />
			<specular r="0.1" g="0.1" b="0.1" a="0.1" />
		</spot>

		<spot id="light5" enabled="0" angle="0.5" exponent="1.0" >
			<location x="0" y="0" z="0" w="0" />
			<target x="0" y="0" z="0" />
			<ambient r="0.1" g="0.1" b="0.1" a="0.1" />
			<diffuse r="0.1" g="0.1" b="0.1" a="0.1" />
			<specular r="0.1" g="0.1" b="0.1" a="0.1" />
		</spot>
	</lights>

	<textures> 
		<texture id="bank" file="scenes/images/bank.jpg"/>
		<texture id="leaves" file="scenes/images/leaves.jpg"/> 
		<texture id="rocks" file="scenes/images/rocks.jpg"/> 
		<texture id="trunk" file="scenes/images/trunk-image.jpg"/> 
		<texture id="vidral" file="scenes/images/vidral.jpg"/>
	</textures> 

	<materials>
		<material id="mat0" shininess="100.0">
			<emission r="0.3" g="0.3" b="0.3" a="1.0" />
			<ambient r="0.6" g="0.6" b="0.6" a="1.0" />
			<diffuse r="0.8" g="0.8" b="0.8" a="1.0" />
			<specular r="0.8" g="0.8" b="0.8" a="1.0" />
		</material>

		<material id="mat1" shininess="120.0">
			<emission r="0.3" g="0.3" b="0.3" a="1.0" />
			<ambient r="0.5" g="0.5" b="0.5" a="1.0" />
			<diffuse r="0.4" g="1.0" b="0.8" a="1.0" />
			<specular r="0.1" g="0.1" b="0.1" a="1.0" />
		</material>

		<material id="mat2" shininess="120.0">
			<emission r="0.3" g="0.3" b="0." a="1.0" />
			<ambient r="0.6" g="0.6" b="0.9" a="1.0" />
			<diffuse r="0.8" g="0.8" b="0.9" a="1.0" />
			<specular r="0.1" g="0.1" b="0.1" a="1.0" />
		</material>

		<material id="mat3" shininess="120.0">
			<emission r="0.3" g="0.3" b="0.3" a="1.0" />
			<ambient r="0.6" g="0.6" b="0.6" a="1.0" />
			<diffuse r="0.8" g="0.8" b="0.8" a="1.0" />
			<specular r="0.8" g="0.8" b="0.8" a="1.0" />
		</material>

		<material id="mat4" shininess="120.0">
			<emission r="0.3" g="0.3" b="0.3" a="1.0" />
			<ambient r="0.6" g="0.6" b="0.6" a="1.0" />
			<diffuse r="0.8" g="0.8" b="0.8" a="1.0" />
			<specular r="0.8" g="0.8" b="0.8" a="1.0" />
		</material>
	</materials>

	<transformations>

		<!-- Front Cube Face -->
		<transformation id="front_cube"> 
			<translate x="0.0" y="0.0" z="0.5" />
		</transformation> 
 
 		<!-- Back Cube Face -->
		<transformation id="back_cube"> 
			<rotate axis="x" angle="180.0" />
			<translate x="0.0" y="0.0" z="0.5" />
		</transformation> 

		<!-- Right Cube Face -->
		<transformation id="right_cube"> 
			<rotate axis="y" angle="90.0" />
			<translate x="0.0" y="0.0" z="0.5" /> 
		</transformation> 
 
 		<!-- Left Cube Face -->
		<transformation id="left_cube"> 
			<rotate axis="y" angle="-90.0" /> 
			<translate x="0.0" y="0.0" z="0.5" /> 
		</transformation> 
 
 		<!-- Top Cube Face -->
		<transformation id="top_cube"> 
			<rotate axis="x" angle="-90.0" />
			<translate x="0.0" y="0.0" z="0.5" />
		</transformation> 
 
		<!-- Bottom Cube Face -->
		<transformation id="bot_cube"> 
			<rotate axis="x" angle="90.0" /> 
			<translate x="0.0" y="0.0" z="0.5" />
		</transformation> 

		<!-- Table Top -->
		<transformation id="top_table"> 
			<translate x="0.0" y="3.6" z="0.0" />
			<scale x="5.0" y="0.3" z="3.0" />
		</transformation> 

		<!-- Table Front Left Leg -->
		<transformation id="front_left_table"> 
			<translate x="-2.3" y="1.75" z="1.3" />
			<scale x="0.3" y="3.5" z="0.3" />
		</transformation>

		<!-- Table Front Right Leg -->
		<transformation id="front_right_table"> 
			<translate x="-2.3" y="1.75" z="-1.3" />
			<scale x="0.3" y="3.5" z="0.3" />
		</transformation>

		<!-- Table Back Left Leg -->
		<transformation id="back_left_table"> 
			<translate x="2.3" y="1.75" z="1.3" />
			<scale x="0.3" y="3.5" z="0.3" />
		</transformation>

		<!-- Table Back Right Leg -->
		<transformation id="back_right_table"> 
			<translate x="2.3" y="1.75" z="-1.3" />
			<scale x="0.3" y="3.5" z="0.3" />
		</transformation> 

		<!-- Table transform -->
		<transformation id="total_table">
			<translate x="8.0" y="0.0" z="8.0" />
		</transformation>

		<!-- XY axis Wall -->
		<transformation id="wall0">
			<translate x="10.0" y="5.0" z="0.0" />
			<scale x="20.0" y="10.0" z="1.0" />
		</transformation>

		<!-- YZ axis Wall -->
		<transformation id="wall1">
			<rotate axis="y" angle="-90.0" />
			<rotate axis="x" angle="180.0" /> 
			<translate x="10.0" y="-5.0" z="0.0" />
			<scale x="20.0" y="10.0" z="1.0" />
		</transformation>

		<transformation id="bed0">
			<scale x="4.0" y="1.5" z="8.0" />
			<translate x="0.625" y="0.6" z="0.525" />
		</transformation>

		<transformation id="bed1">
			<scale x="5.0" y="0.2" z="8.5" />
			<translate x="0.5" y="0.5" z="0.5" />
		</transformation>

		<transformation id="bed2">
			<scale x="5.0" y="3.5" z="0.2" />
			<translate x="0.5" y="0.5" z="0.5" />
		</transformation>

		<transformation id="pillow">
			<scale x="3.0" y="0.5" z="1.5" />
			<translate x="0.85" y="3.5" z="0.7" />
		</transformation>

		<transformation id="cylinder">
			<rotate axis="x" angle="-90.0" />
			<translate x="5.0" y="-14.0" z="0.0" />
		</transformation>
	</transformations> 

	<primitives>

		<primitive id="rectangle">
			<rectangle x1="-0.5" y1="-0.5" x2="0.5" y2="0.5" />
		</primitive>

		<primitive id="triangle">
			<triangle x1="0.5" y1="0.0" z1="0.0" x2="-0.5" y2="0.0" z2="0.0" x3="0.0" y3="0.0" z3="1.0" />
		</primitive>
		
		<primitive id="cylinder">
			<cylinder base="2.0" top="1.0" height="4.0" slices="20" stacks="20" />
		</primitive>
		
		<primitive id="sphere">
			<sphere radius="1.0" slices="20" stacks="20" />
		</primitive>
		
		<primitive id="torus">
			<torus inner="0.5" outer="0.5" slices="20" loops="20" />
		</primitive>

	</primitives>

	<components> 

		<!-- cylinder Attempt -->
		<component id="cylinder"> 
			<transformation> 
				<transformationref id="cylinder" />
			</transformation> 

			<materials>
				<material id="inherit" />
			</materials>

			<texture id="leaves" length_s="1.0" length_t="1.0" />
 
			<children> 
				<primitiveref id="cylinder" /> 
			</children> 
		</component>

		<!-- Wall -->
		<component id="wall0"> 
			<transformation> 
				<transformationref id="wall0" />
			</transformation> 

			<materials>
				<material id="inherit" />
			</materials>

			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<primitiveref id="rectangle" /> 
			</children> 
		</component>
		<component id="wall1"> 
			<transformation> 
				<transformationref id="wall1" />
			</transformation> 

			<materials>
				<material id="inherit" />
			</materials>

			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<primitiveref id="rectangle" /> 
			</children> 
		</component>
		<component id="walls"> 
			<transformation>
			</transformation>

			<materials> 
				<material id="mat0" />
			</materials> 
 
			<texture id="rocks" length_s="1.0" length_t="1.0" />
 
			<children> 
				<componentref id="wall0" />
				<componentref id="wall1" />
			</children> 
		</component>



		<!-- Cube -->
		<component id="cube0"> 
			<transformation> 
				<transformationref id="front_cube" />
			</transformation> 

			<materials>
				<material id="inherit" />
			</materials>

			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<primitiveref id="rectangle" /> 
			</children> 
		</component>
		<component id="cube1"> 
			<transformation> 
				<transformationref id="back_cube" />
			</transformation> 

			<materials>
				<material id="inherit" />
			</materials>

			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<primitiveref id="rectangle" /> 
			</children> 
		</component>
		<component id="cube2"> 
			<transformation> 
				<transformationref id="right_cube" />
			</transformation> 

			<materials>
				<material id="inherit" />
			</materials>

			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<primitiveref id="rectangle" /> 
			</children> 
		</component>
		<component id="cube3"> 
			<transformation> 
				<transformationref id="left_cube" />
			</transformation> 

			<materials>
				<material id="inherit" />
			</materials>

			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<primitiveref id="rectangle" /> 
			</children> 
		</component>
		<component id="cube4"> 
			<transformation> 
				<transformationref id="top_cube" />
			</transformation> 

			<materials>
				<material id="inherit" />
			</materials>
 
			<texture id="inherit" length_s="1.0" length_t="1.0" />
			
			<children> 
				<primitiveref id="rectangle" /> 
			</children> 
		</component>
		<component id="cube5"> 
			<transformation> 
				<transformationref id="bot_cube" />
			</transformation> 

			<materials>
				<material id="inherit" />
			</materials>

			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<primitiveref id="rectangle" /> 
			</children> 
		</component>
		<component id="cube"> 
			<transformation>
			</transformation>

			<materials> 
				<material id="leaves" />
			</materials> 
 
			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<componentref id="cube0" />
				<componentref id="cube1" />
				<componentref id="cube2" />
				<componentref id="cube3" />
				<componentref id="cube4" />
				<componentref id="cube5" />
			</children> 
		</component>



		<!-- Table -->
		<component id="table0">
			<transformation>
				<transformationref id="top_table" />
			</transformation>

			<materials> 
				<material id="inherit" />
			</materials> 
 
			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<componentref id="cube" />
			</children> 
		</component>
		<component id="table1"> 
			<transformation>
				<transformationref id="front_left_table" />
			</transformation>

			<materials> 
				<material id="inherit" />
			</materials> 
 
			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<componentref id="cube" />
			</children> 
		</component>
		<component id="table2"> 
			<transformation>
				<transformationref id="front_right_table" />
			</transformation>

			<materials> 
				<material id="inherit" />
			</materials> 
 
			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<componentref id="cube" />
			</children> 
		</component>
		<component id="table3"> 
			<transformation>
				<transformationref id="back_left_table" />
			</transformation>

			<materials> 
				<material id="inherit" />
			</materials> 
 
			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<componentref id="cube" />
			</children> 
		</component>
		<component id="table4"> 
			<transformation>
				<transformationref id="back_right_table" />
			</transformation>

			<materials> 
				<material id="inherit" />
			</materials> 
 
			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<componentref id="cube" />
			</children> 
		</component>
		<component id="table"> 
			<transformation>
				<transformationref id="total_table" />
			</transformation>

			<materials> 
				<material id="mat0" />
			</materials> 
 
			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<componentref id="table0" />
				<componentref id="table1" />
				<componentref id="table2" />
				<componentref id="table3" />
				<componentref id="table4" />
			</children> 
		</component>



		<!-- Bed -->
		<component id="bed0"> 
			<transformation>
				<transformationref id="bed0" />
			</transformation>

			<materials> 
				<material id="inherit" />
			</materials> 
 
			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<componentref id="cube" />
			</children> 
		</component>
		<component id="bed1"> 
			<transformation>
				<transformationref id="bed1" />
			</transformation>

			<materials> 
				<material id="inherit" />
			</materials> 
 
			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<componentref id="cube" />
			</children> 
		</component>
		<component id="bed2"> 
			<transformation>
				<transformationref id="bed2" />
			</transformation>

			<materials> 
				<material id="inherit" />
			</materials> 
 
			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<componentref id="cube" />
			</children> 
		</component>
		<component id="pillow"> 
			<transformation>
				<transformationref id="pillow" />
			</transformation>

			<materials> 
				<material id="inherit" />
			</materials> 
 
			<texture id="inherit" length_s="1.0" length_t="1.0" />
 
			<children> 
				<componentref id="cube" />
			</children> 
		</component>
		<component id="bed"> 
			<transformation>
			</transformation>

			<materials> 
				<material id="mat0" />
			</materials> 
 
			<texture id="trunk" length_s="1.0" length_t="1.0" />
 
			<children> 
				<componentref id="bed0" />
				<componentref id="bed1" />
				<componentref id="bed2" />
				<componentref id="pillow" />
			</children> 
		</component>


		<!-- Root Component -->
		<component id="child_room">
			<transformations>
			</transformations>

			<materials>
				<material id="mat0" />
			</materials>

			<texture id="leaves" length_s="1.0" length_t="1.0" />

			<children> 
				<componentref id="table" />
				<componentref id="walls" />
				<componentref id="bed" />
				<componentref id="cylinder" />
			</children> 
		</component>
	</components> 
 
</yas>